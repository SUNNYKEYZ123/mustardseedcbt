    <button id="backToTop" class="fixed bottom-6 right-6 bg-blue-600 text-white p-2 md:p-3 rounded-full shadow-lg transition-all duration-300 hover:bg-blue-700 touch-target opacity-100 visible">
        <i class="fas fa-arrow-up"></i>
